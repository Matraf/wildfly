<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">

    <!-- [wildfly] Apache ActiveMQ is not shipped with wildfly ('ActiveMQ Artemis' is not 'ActiveMQ') -->
    <suppress>
        <notes><![CDATA[
   file name: activemq-artemis-native-1.0.2.jar
   file name: artemis-wildfly-jakarta-integration-1.0.5.jar (shaded: org.apache.activemq.tests:unit-tests:2.18.0)
   ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.apache\.activemq(/activemq\-artemis\-native|\.tests/.*)@.*$</packageUrl>
        <cve>CVE-2014-3576</cve>
        <cve>CVE-2016-3088</cve>
        <cve>CVE-2018-11775</cve>
    </suppress>

    <!-- [wildfly] Used only in tests, not deployed  -->
    <suppress>
        <notes><![CDATA[
	   file name: apacheds-all-2.0.0-M24.jar (shaded: org.apache.directory.api:api-ldap-net-mina:1.0.0)
	   ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.apache\.directory\.api/api\-.*@.*$</packageUrl>
        <cve>CVE-2015-3250</cve>
        <cve>CVE-2018-1337</cve>
    </suppress>

    <!-- Vulnerabilities do not apply to xjc plugins -->
    <suppress>
        <notes><![CDATA[
	   file name: cxf-xjc-dv-3.3.1.jar
	   file name: cxf-xjc-ts-3.3.1.jar
	   file name: cxf-xjc-bug986-3.3.1.jar
	   file name: cxf-xjc-boolean-3.3.1.jar
	   ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.apache\.cxf\.xjcplugins/cxf\-xjc\-(dv|ts|bug986|boolean)@.*$</packageUrl>
        <cve>CVE-2019-12406</cve>
        <cve>CVE-2019-12419</cve>
        <cve>CVE-2019-12423</cve>
        <cve>CVE-2019-17573</cve>
    </suppress>

    <!-- [wildfly-core] The vulnerability applies to io.undertow package, undertow-server from org.wildfly.security.elytron-web package is a different module -->
    <suppress>
        <notes><![CDATA[
       file name: undertow-server-1.9.1.Final.jar
       ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.wildfly\.security\.elytron\-web/undertow\-server.*@.*$</packageUrl>
        <cve>CVE-2019-3888</cve>
        <cve>CVE-2019-10184</cve>
        <cve>CVE-2019-10212</cve>
        <cve>CVE-2019-14888</cve>
        <cve>CVE-2019-19343</cve>
        <cve>CVE-2020-1745</cve>
        <cve>CVE-2020-1757</cve>
        <cve>CVE-2020-10705</cve>
        <cve>CVE-2020-10719</cve>
    </suppress>

    <!-- This is a false positive because wildfly-core itself does not use mockserver libraries. Mockserver is used only for test purposes (wildfly-core-testsuite-shared module). -->
    <suppress>
        <notes><![CDATA[
   file name: mockserver-core-5.8.1.jar
   ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.mock\-server/mockserver\-.*$</packageUrl>
        <cve>CVE-2021-32827</cve>
    </suppress>

    <!-- wildfly 25.0.0 is shipped with netty 4.1.68.Final and the suppressed CVEs relate to the versions below 4.1.60. Furthermore, netty version 4.1.43 comes from mockserver dependency, which is a part of a testsuite.  -->
    <suppress>
        <notes><![CDATA[
   file name: netty-buffer-4.1.43.Final.jar
   file name: netty-codec-4.1.43.Final.jar
   file name: netty-common-4.1.43.Final.jar
   file name: netty-handler-4.1.43.Final.jar
   file name: netty-transport-4.1.43.Final.jar
   ]]></notes>
        <packageUrl regex="true">^pkg:maven/io\.netty/netty\-.*$</packageUrl>
        <cve>CVE-2019-20444</cve>
        <cve>CVE-2019-20445</cve>
        <cve>CVE-2020-11612</cve>
        <cve>CVE-2020-7238</cve>
    </suppress>

</suppressions>